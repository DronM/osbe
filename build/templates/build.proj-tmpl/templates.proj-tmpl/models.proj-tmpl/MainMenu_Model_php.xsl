<?xml version="1.0" encoding="UTF-8"?><xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform"><xsl:output method="text" indent="yes"			doctype-public="-//W3C//DTD XHTML 1.0 Strict//EN" 			doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/>			<xsl:template match="mainMenu"><![CDATA[<?php]]>require_once(FRAME_WORK_PATH.'basic_classes/Model.php');class MainMenu_Model_<xsl:value-of select="$ROLE_ID"/> extends Model{	public function dataToXML(){		return '&lt;model id="MainMenu_Model" sysModel="1"&gt;		<xsl:apply-templates select="menuItem"/>		&lt;/model&gt;';	}}<![CDATA[?>]]></xsl:template><xsl:template match="menuItem">&lt;menuItem<xsl:if test="@viewId">viewId="<xsl:value-of select="@viewId"/>"</xsl:if>descr="<xsl:value-of select="@descr"/>"<xsl:if test="@default">default="<xsl:value-of select="@default"/>"</xsl:if><xsl:if test="@c">c="<xsl:value-of select="@c"/>"</xsl:if><xsl:if test="@f">f="<xsl:value-of select="@f"/>"</xsl:if><xsl:if test="@v">v="<xsl:value-of select="@v"/>"</xsl:if><xsl:if test="@t">t="<xsl:value-of select="@t"/>"</xsl:if><xsl:if test="@limit">limit="<xsl:value-of select="@limit"/>"</xsl:if><xsl:if test="@count">count="<xsl:value-of select="@count"/>"</xsl:if>&gt;<xsl:apply-templates select="menuItem"/>&lt;/menuItem&gt;</xsl:template></xsl:stylesheet>